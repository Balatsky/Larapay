{
  "name": "nxmad/larapay",
  "description": "Powerful multi-gateway payment processing library designed to make life easier in Laravel 5",
  "keywords": [
    "skylex",
    "wallet",
    "laravel",
    "larapay",
    "payments",
    "transactions"
  ],
  "license": "MIT",
  "homepage": "https://skylex.pro/dev/skylex",
  "support": {
    "issues": "https://github.com/nxmad/Larapay/issues",
    "source": "https://github.com/nxmad/Larapay"
  },
  "authors": [
    {
      "name": "Alex Balatsky",
      "email": "alex@skylex.pro"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "illuminate/http": "^5.0",
    "illuminate/config": "^5.0",
    "illuminate/support": "^5.0",
    "illuminate/database": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Nxmad\\Larapay\\": "src"
    },

    "files": [
      "files/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nxmad\\Larapay\\LarapayServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests/"
  }
}
